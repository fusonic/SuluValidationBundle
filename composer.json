{
    "name": "sulu/validation-bundle",
    "type": "symfony-bundle",
    "description": "This bundle is for validation of schemas",
    "license": "MIT",
    "keywords": ["sulu", "schema", "validation"],
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "https://github.com/sulu/SuluValidationBundle/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sulu\\Bundle\\ValidationBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sulu\\Bundle\\ValidationBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.3",
        "symfony/framework-bundle": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "symfony/finder": "^4.4 || ^5.0",
        "symfony/yaml": "^4.4 || ^5.0",
        "phpunit/phpunit": "^8.5",
        "symfony/browser-kit": "^4.4 || ^5.0",
        "symfony/console": "^4.4 || ^5.0",
        "phpstan/phpstan": "^0.12"
    },
    "scripts": {
        "phpstan": "php vendor/bin/phpstan analyse src --level=5"
    }
}
